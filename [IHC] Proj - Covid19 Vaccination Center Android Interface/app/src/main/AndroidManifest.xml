<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="android.example.covid19app">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Covid19App">
        <activity
            android:name=".Vacinas"
            android:parentActivityName=".Menu"/>
        <activity
            android:name=".Selecionar_grupo"
            android:parentActivityName=".Menu" />
        <activity
            android:name=".Pessoas_grupo"
            android:parentActivityName=".Verificar_Confirmados" />
        <activity
            android:name=".Pessoas_grupo2"
            android:parentActivityName=".Verificar_Confirmados"
            tools:ignore="DuplicateActivity" />
        <activity
            android:name=".Pessoas_grupo3"
            android:parentActivityName=".Verificar_Confirmados" />
        <activity
            android:name=".Verificar_Confirmados"
            android:parentActivityName=".Menu" />
        <activity android:name=".Menu" />
        <activity android:name=".Register" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>